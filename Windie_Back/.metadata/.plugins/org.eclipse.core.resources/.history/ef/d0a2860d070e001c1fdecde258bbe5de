package modelo;

import java.sql.SQLException;

public class DesenvolvedorDAO {

	private static DesenvolvedorDAO instance;
	
	
	
	public static DesenvolvedorDAO getInstance() throws SQLException {
		
		if(instance ==null) {
			
			instance = new DesenvolvedorDAO();
			return instance;
			
		}else {
			return instance;
		}
	}
}
